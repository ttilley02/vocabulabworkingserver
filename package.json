{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "deploy": "git push heroku master",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "postdeploy": "npm run migrate:production",
    "predeploy": "npm audit && npm run migrate:production"
  },
  "keywords": [],
  "engines": {
    "node": "v12.14.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "postgrator": "^3.2.0",
    "treeize": "^2.1.2",
    "uuid": "^8.0.0",
    "winston": "^3.2.1",
    "xss": "^1.0.8",
    "nodemon": "^2.0.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.7",
    "postgrator-cli": "^3.1.0",
    "supertest": "^4.0.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ttilley02/server-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/ttilley02/server-boilerplate/issues"
  },
  "homepage": "https://github.com/ttilley02/server-boilerplate#readme",
  "description": ""
}
